CREATE OR REPLACE VIEW stg_prodvols_pra_network_01_major_product_oil FOLDER = '/03. staging views/production/prodvols/stg_prodvols_pra_network' AS SELECT DISTINCT stg_sap_pra_delivery_network_calc_method.dn_number AS dn_number, 'YES' AS major_product_oil FROM stg_sap_pra_delivery_network_calc_method WHERE (eff_from_date <= current_date() AND eff_to_date > current_date() AND major_product_code = 1);

ALTER VIEW stg_prodvols_pra_network_01_major_product_oil
 LAYOUT (stg_sap_pra_delivery_network_calc_method = [20, 20, 200, 105]);

