CREATE OR REPLACE VIEW stg_prodvols_pra_network_01_major_product_code FOLDER = '/03. staging views/production/prodvols/stg_prodvols_pra_network' AS SELECT stg_sap_pra_delivery_network_calc_method.dn_number AS dn_number, min(stg_sap_pra_delivery_network_calc_method.major_product_code) AS major_product_code FROM stg_sap_pra_delivery_network_calc_method WHERE (eff_from_date <= current_date() AND eff_to_date > current_date()) GROUP BY stg_sap_pra_delivery_network_calc_method.dn_number;

ALTER VIEW stg_prodvols_pra_network_01_major_product_code
 LAYOUT (stg_sap_pra_delivery_network_calc_method = [20, 20, 200, 105]);

